{
  "schema_version": 1,
  "id": "google-image-generation",
  "provider": "google",
  "provider_display_name": "Google",
  "provider_url": "ai.google.dev",
  "name": "Google Imagen Image Generation",
  "description": "Text-to-image generation via Google Imagen 4 (Gemini API)",
  "auth": {
    "type": "header",
    "header": "x-goog-api-key",
    "prefix": "",
    "env_key": "GOOGLE_API_KEY"
  },
  "request": {
    "method": "POST",
    "url": "https://generativelanguage.googleapis.com/v1beta/models/{model}:predict",
    "content_type": "application/json",
    "body_template": {
      "instances": [
        {}
      ],
      "parameters": {}
    },
    "params": [
      {
        "name": "model",
        "type": "enum",
        "options": [
          "imagen-4.0-generate-001",
          "imagen-4.0-fast-generate-001",
          "imagen-4.0-ultra-generate-001"
        ],
        "default": "imagen-4.0-generate-001",
        "ui": "dropdown",
        "required": true,
        "url_path": true
      },
      {
        "name": "prompt",
        "type": "string",
        "ui": "textarea",
        "required": true,
        "placeholder": "A cat astronaut floating in space...",
        "body_path": "instances.0.prompt"
      },
      {
        "name": "aspectRatio",
        "type": "enum",
        "options": [
          "1:1",
          "3:4",
          "4:3",
          "9:16",
          "16:9"
        ],
        "default": "1:1",
        "ui": "dropdown",
        "body_path": "parameters.aspectRatio"
      },
      {
        "name": "sampleCount",
        "type": "integer",
        "min": 1,
        "max": 4,
        "default": 1,
        "ui": "slider",
        "body_path": "parameters.sampleCount",
        "group": "advanced"
      }
    ]
  },
  "interaction": {
    "pattern": "sync"
  },
  "examples": [
    {
      "label": "Space cat",
      "params": {
        "prompt": "A cat astronaut floating in space, photorealistic, 8k, dramatic lighting",
        "model": "imagen-4.0-generate-001"
      }
    },
    {
      "label": "Mountain sunset",
      "params": {
        "prompt": "A mountain landscape at sunset, oil painting style, vibrant colors",
        "model": "imagen-4.0-generate-001"
      }
    },
    {
      "label": "Cyberpunk city",
      "params": {
        "prompt": "A futuristic cyberpunk city at night, neon signs, rain-soaked streets, blade runner style",
        "model": "imagen-4.0-fast-generate-001"
      }
    }
  ],
  "response": {
    "outputs": [
      {
        "path": "$.predictions[0].bytesBase64Encoded",
        "type": "image",
        "source": "base64",
        "mime_type": "image/png",
        "downloadable": true
      }
    ],
    "error": {
      "path": "$.error.message"
    }
  }
}
