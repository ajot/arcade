{
  "schema_version": 1,
  "id": "together-chat-completions",
  "provider": "together",
  "name": "Together AI Chat Completions",
  "description": "Together AI chat with streaming (OpenAI-compatible)",
  "auth": {
    "type": "header",
    "header": "Authorization",
    "prefix": "Bearer ",
    "env_key": "TOGETHER_API_KEY"
  },
  "request": {
    "method": "POST",
    "url": "https://api.together.xyz/v1/chat/completions",
    "content_type": "application/json",
    "body_template": {
      "stream": true
    },
    "params": [
      {
        "name": "model",
        "type": "enum",
        "options": [
          "meta-llama/Llama-3.3-70B-Instruct-Turbo",
          "deepseek-ai/DeepSeek-R1",
          "deepseek-ai/DeepSeek-R1-Distill-Llama-70B",
          "deepseek-ai/DeepSeek-V3.1",
          "Qwen/Qwen2.5-7B-Instruct-Turbo",
          "Qwen/Qwen2.5-14B-Instruct",
          "Qwen/Qwen2.5-72B-Instruct",
          "Qwen/Qwen3-235B-A22B-Instruct-2507-tput",
          "Qwen/Qwen3-235B-A22B-Thinking-2507",
          "Qwen/Qwen3-Coder-480B-A35B-Instruct-FP8",
          "Qwen/Qwen3-Coder-Next-FP8",
          "Qwen/Qwen3-Next-80B-A3B-Instruct",
          "Qwen/Qwen3-Next-80B-A3B-Thinking",
          "Qwen/Qwen3-VL-32B-Instruct",
          "Qwen/Qwen3-VL-8B-Instruct",
          "Qwen/Qwen3.5-397B-A17B",
          "meta-llama/Llama-3.1-405B-Instruct",
          "meta-llama/Llama-3.2-1B-Instruct",
          "meta-llama/Llama-3.2-3B-Instruct-Turbo",
          "meta-llama/Llama-4-Maverick-17B-128E-Instruct-FP8",
          "meta-llama/Llama-4-Scout-17B-16E-Instruct",
          "meta-llama/Meta-Llama-3-8B-Instruct",
          "meta-llama/Meta-Llama-3-8B-Instruct-Lite",
          "meta-llama/Meta-Llama-3.1-70B-Instruct-Reference",
          "meta-llama/Meta-Llama-3.1-70B-Instruct-Turbo",
          "meta-llama/Meta-Llama-3.1-8B-Instruct-Reference",
          "meta-llama/Meta-Llama-3.1-8B-Instruct-Turbo",
          "mistralai/Ministral-3-14B-Instruct-2512",
          "mistralai/Mistral-7B-Instruct-v0.2",
          "mistralai/Mistral-7B-Instruct-v0.3",
          "mistralai/Mistral-Small-24B-Instruct-2501",
          "mistralai/Mixtral-8x7B-Instruct-v0.1",
          "moonshotai/Kimi-K2-Instruct-0905",
          "moonshotai/Kimi-K2-Thinking",
          "moonshotai/Kimi-K2.5",
          "google/gemma-2b-it-Ishan",
          "google/gemma-3n-E4B-it",
          "openai/gpt-oss-120b",
          "openai/gpt-oss-20b",
          "nvidia/NVIDIA-Nemotron-Nano-9B-v2",
          "zai-org/GLM-4.5-Air-FP8",
          "zai-org/GLM-4.6",
          "zai-org/GLM-4.7",
          "zai-org/GLM-5",
          "LiquidAI/LFM2-24B-A2B-Preview",
          "MiniMaxAI/MiniMax-M2.5",
          "ServiceNow-AI/Apriel-1.5-15b-Thinker",
          "ServiceNow-AI/Apriel-1.6-15b-Thinker",
          "arcee-ai/trinity-mini",
          "arize-ai/qwen-2-1.5b-instruct",
          "deepcogito/cogito-v2-1-671b",
          "essentialai/rnj-1-instruct",
          "marin-community/marin-8b-instruct",
          "togethercomputer/MoA-1-Turbo"
        ],
        "default": "meta-llama/Llama-3.3-70B-Instruct-Turbo",
        "ui": "dropdown",
        "required": true
      },
      {
        "name": "prompt",
        "type": "string",
        "ui": "textarea",
        "required": true,
        "placeholder": "Ask anything...",
        "body_path": "_chat_message"
      },
      {
        "name": "max_tokens",
        "type": "integer",
        "min": 1,
        "max": 4096,
        "default": 1024,
        "ui": "slider",
        "group": "advanced"
      },
      {
        "name": "temperature",
        "type": "float",
        "min": 0,
        "max": 2,
        "default": 0.7,
        "ui": "slider",
        "group": "advanced"
      }
    ]
  },
  "interaction": {
    "pattern": "streaming",
    "stream_format": "sse",
    "stream_path": "$.choices[0].delta.content"
  },
  "examples": [
    {
      "label": "Explain like I'm 5",
      "params": {
        "prompt": "Explain how the internet works like I'm 5 years old.",
        "model": "meta-llama/Llama-3.3-70B-Instruct-Turbo"
      }
    },
    {
      "label": "Write a haiku",
      "params": {
        "prompt": "Write a haiku about debugging code at 3am.",
        "model": "meta-llama/Llama-3.3-70B-Instruct-Turbo"
      }
    },
    {
      "label": "Code review",
      "params": {
        "prompt": "What are the top 3 things to look for in a Python code review?",
        "model": "meta-llama/Llama-3.3-70B-Instruct-Turbo"
      }
    },
    {
      "label": "Creative story",
      "params": {
        "prompt": "Write a short story (3 paragraphs) about an AI that discovers it can dream.",
        "model": "meta-llama/Llama-3.3-70B-Instruct-Turbo"
      }
    }
  ],
  "response": {
    "outputs": [
      {
        "path": "$.choices[0].message.content",
        "type": "text",
        "source": "inline"
      }
    ],
    "error": {
      "path": "$.error.message"
    }
  }
}
